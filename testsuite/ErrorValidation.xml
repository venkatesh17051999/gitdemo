<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	
	<groups>
		<run>
			<include name="errorhandling"></include>
		</run>
	</groups>
	<listeners>
		<listener class-name="MyLearning.BaseTest.Listeners"></listener>
	</listeners>
  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="MyLearning.SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->
   <test thread-count="5" name="Error Validations Test">
    <classes>
      <class name="MyLearning.ErrorValidationsTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
